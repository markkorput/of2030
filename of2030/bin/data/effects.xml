<of2030>
  <effects>

    <!-- inloop -->
    <effect name="cursor">
      <pattern>cursor</pattern>
      <!-- real world meter -->
      <gain>0.1</gain>
      <pano_velocity>0.5</pano_velocity>
    </effect>

    <effect name="worms1">
      <shader>worms</shader>
      <pano_start>0.0</pano_start>
      <pano_end>1.0</pano_end>
      <!-- <tunnel_start>0.5</tunnel_start>
      <tunnel_end>0.9</tunnel_end> -->
      <gain>0.65</gain>
      <size_x>68.0</size_x>
      <size_y>1.0</size_y>
    </effect>

    <!-- roof -->
    <effect name="roof">
      <mask_coords_name>roof_coord</mask_coords_name>
      <color_r>255</color_r>
      <color_g>255</color_g>
      <color_b>0</color_b>
    </effect>

    <!-- voice -->
    <effect name="voice">
      <mask_coords_name>voice_coord</mask_coords_name>
    </effect>

    <!-- pos -->
    <effect name="pos">
      <pattern>pos</pattern>
    </effect>

    <!-- spot -->
    <effect name="spot1">
      <number>1</number>
    </effect>

    <effect name="spot2">
      <number>2</number>
    </effect>

    <effect name="spot3">
      <number>3</number>
    </effect>

    <effect name="spot">
