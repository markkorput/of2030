<of2030>
  <effects>
    <!--
    <effect name="<effect_name>[.<song>][.<clip>][.<trigger>]"
    <effect name="shader-spotlight.fusion.fusion2.spotlight_max"
    Effect configs are loaded in the following order (every newly loaded config
    adds to/overwrites earlier configs, so later configs have higher priority):
    - <effect_name>
    - <effect_name>.<song_name>
    - <effect_name>.<song_name>.<clip_name>
    - <effect_name>.<trigger_name>
    - <effect_name>.<song_name>.<clip_name>.<trigger_name>
    -->

    <!-- inloop -->
    <effect name="worms">
      <gain>0.0</gain>
      <duration>30.0</duration>
    </effect>

    <effect name="worms.worms1">
      <shader>worms</shader>
      <duration>210</duration>
      <pano_start>0.162</pano_start>
      <pano_end>0.324</pano_end>
    </effect>

    <effect name="worms.worms2">
      <shader>worms</shader>
      <duration>210</duration>
      <pano_start>0.5</pano_start>
      <pano_end>0.662</pano_end>
    </effect>

    <!-- STELLAR -->
    <effect name="spot.spot1">
      <shader>spot</shader>
      <gain>0.2</gain>
      <pos_x>0.25</pos_x>
      <pos_y>0.01</pos_y>
    </effect>

    <effect name="worms.worms11">
      <shader>worms</shader>
      <duration>5.0</duration>
      <gain>1.0</gain>
      <pano_start>0.162</pano_start>
      <pano_end>0.162</pano_end>
      <pos_x>10.3</pos_x>
      <pos_y>0.3</pos_y>
    </effect>

    <effect name="worms.worms12">
      <shader>worms</shader>
      <duration>5.0</duration>
      <gain>1.0</gain>
      <pano_start>0.162</pano_start>
      <pano_end>0.162</pano_end>
      <pos_x>10.3</pos_x>
      <pos_y>0.8</pos_y>
    </effect>

    <!-- debug -->
    <effect name="test">
      <shader>worms</shader>
      <gain>0.5</gain>
    </effect>

    <effect name="cursor">
      <shader>cursor</shader>
      <duration>3.0</duration>
      <iterations>1.4</iterations>
      <gain>100.0</gain>
    </effect>

  </effects>
</of2030>
